<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<set name="MultiCultivator">
    <include>userland/core</include>
    <include>multi-cultivator/time</include>
    <include>multi-cultivator/thermo</include>
    <include>multi-cultivator/protocol</include>
    <include>common/core</include>
    <include>common/system</include>
    <include>userland/events</include>
    <include>multi-cultivator/lights</include>
    
    <command name="get-water-level" id="1110">
        <descr>1 - OK, 0 - low</descr>
        <return>110</return>
    </command>
    
    <command name="get-airpump-state" id="1120">
        <descr>1 - ON, 0 - OFF</descr>
        <return>110</return>
    </command>
    
    <command name="set-airpump-state" id="1121">
        <param name="state" type="UINT8">
            <descr>1 - ON, 0 - OFF</descr>
        </param>
        <return>100</return>
    </command>
</set>
